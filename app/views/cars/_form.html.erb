<div class="center_tab">
<%= form_for(:car) do |f| %>

  <div class="field">
    <%= f.label :registration_number %><br>
    <%= f.text_field :registration_number %>
  </div>

  <div class="field">
    <%= f.label :color %><br>
    <%= f.text_field :color %>
  </div>

  <div class="button">
    <%= f.submit "Print Ticket" %>
  </div>
<%end%>
</div>

<div class="new_button">
<%= link_to 'Back To Home Page', "/cars" %>
</div>
